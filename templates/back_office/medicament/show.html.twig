{% extends 'back_office/base.html.twig' %}



{% block content %}
    <body class="kt-quick-panel--right kt-demo-panel--right kt-offcanvas-panel--right kt-header--fixed kt-header-mobile--fixed kt-subheader--fixed kt-subheader--enabled kt-subheader--solid kt-aside--enabled kt-aside--fixed kt-page--loading">
    <div class="container">

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ medicament.id }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ medicament.Nom }}</td>
            </tr>
            <tr>
                <th>Type</th>
                <td>{{ medicament.Type }}</td>
            </tr>
            <tr>
                <th>Nb_dose</th>
                <td>{{ medicament.NbDose }}</td>
            </tr>
            <tr>
                <th>Prix</th>
                <td>{{ medicament.Prix }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_medicament_index') }}">
                 <button type="button" class="btn btn-primary">Retourner</button>


<a href="{{ path('app_medicament_edit', {'id': medicament.id}) }}">
             <button type="button" class="btn btn-primary">Modifier</button>
</body>

    {# {{ include('medicament/_delete_form.html.twig') }} #}
{% endblock %}
