{% extends 'base.html.twig' %}

{% block title %}Fiche{% endblock %}

{% block body %}
    <h1>Fiche</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ fiche.id }}</td>
            </tr>
            <tr>
                <th>NomPatient</th>
                <td>{{ fiche.nomPatient }}</td>
            </tr>
            <tr>
                <th>Date_naissance</th>
                <td>{{ fiche.dateNaissance ? fiche.dateNaissance|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ fiche.email }}</td>
            </tr>
            <tr>
                <th>Tel</th>
                <td>{{ fiche.tel }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_fiche_index') }}">back to list</a>

    <a href="{{ path('app_fiche_edit', {'id': fiche.id}) }}">edit</a>

    {{ include('fiche/_delete_form.html.twig') }}
{% endblock %}
